{
  "manifest_version": 3,
  "name": "이미지생성기!!!!!!!!!",
  "version": "1.0",
  "description": "Pixabay api 기반 이미지 검색 확장프로그램",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "downloads",
    "clipboardWrite"
  ],
  "host_permissions": [
    "https://pixabay.com/api/*"
  ]
} 